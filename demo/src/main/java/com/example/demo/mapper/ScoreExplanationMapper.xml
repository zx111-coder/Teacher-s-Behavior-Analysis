<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.ScoreExplanationMapper">
    <insert id="insert" parameterType="com.example.demo.entity.ScoreExplanation">
        INSERT INTO score_explanation (score_id, comment, suggestion, grading_standard,level)
        VALUES (#{scoreId}, #{comment}, #{suggestion}, #{gradingStandard},#{level})
    </insert>

    <select id="findByScoreId" parameterType="java.lang.Long" resultType="com.example.demo.entity.ScoreExplanation">
        SELECT * FROM score_explanation WHERE score_id = #{scoreId}
    </select>
</mapper>